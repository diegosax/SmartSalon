removeLoading();
<%if @events%>
	if ($("#client_calendar").length > 0)
		$("#client_calendar").remove();
	var html = "<%=escape_javascript(render :partial => 'new_event_calendar')%>";
	
	$("#new-event-form").append(html);	
	
<%else%>
	<%if @professionals%>	
		if ($("#choose-professional").length > 0)
			$("#choose-professional").remove();		
		if ($("#client_calendar").length > 0)
			$("#client_calendar").remove();
		var html = "<%=escape_javascript(render :partial => "new_choose_professional")%>";
		$("#new-event-form").append(html);
		$("#choose-professional .chzn_a").chosen().change(function(){			
			onChooseProfessionalChange();
		});
	<%end%>
<%end%>