<div class="row-fluid">
  <div class="switch-board-round">
    <ul class="clearfix">
      <li class="<%=active_action('clients','new')%>">
        <%=link_to new_admin_client_path, :class => "tip-top", :rel => "tooltip", "data-original-title" => "Novo Cliente" do%>  
          <span class="icon-plus"></span>
        <%end%>
      </li>
      <li class="<%=active_action('clients','index')%>">
        <%=link_to admin_clients_path, :class => "tip-top", :rel => "tooltip", "data-original-title" => "Listar Clientes" do%>  
          <span class="icon-list"></span>
        <%end%>
      </li>
      <%if action_name == "show"%>
        <li>
        <%=link_to edit_admin_client_path(@client), :class => "tip-top", :rel => "tooltip", "data-original-title" => "Editar Cliente" do%>  
          <span class="icon-edit"></span>
        <%end%>
      </li>
      <%end%>
   </ul>
 </div>
</div>